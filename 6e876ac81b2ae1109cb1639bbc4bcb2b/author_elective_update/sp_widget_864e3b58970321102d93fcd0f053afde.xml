        <script><![CDATA[function fetchIncidents() {
  var gr = new GlideRecord('incident');
  // Set additional query conditions if needed
  // gr.addQuery('active', true);
  // gr.setLimit(10);
  // gr.orderBy('number');
  gr.query(fetchIncidentsCallback);
}

function fetchIncidentsCallback(response) {
  var table = '<table>' +
    '<tr><th>Number</th><th>Short Description</th><th>Assigned To</th></tr>';

  while (response.next()) {
    var number = response.getValue('number') || '';
    var shortDescription = response.getValue('short_description') || '';
    var assignedTo = response.getDisplayValue('assigned_to') || '';

    table += '<tr>' +
      '<td>' + number + '</td>' +
      '<td>' + shortDescription + '</td>' +
      '<td>' + assignedTo + '</td>' +
      '</tr>';
  }

  table += '</table>';

  document.getElementById('incidentTable').innerHTML = table;
}

// Fetch and display incident data on widget load
fetchIncidents();
]]></script>
